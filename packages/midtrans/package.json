{
  "name": "@indo-payment/midtrans",
  "version": "0.1.0",
  "description": "Midtrans provider for Indo Payment SDK",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
  },
  "keywords": ["payment", "midtrans", "indonesia"],
  "author": "Rohman",
  "license": "MIT",
  "dependencies": {
    "@indo-payment/core": "workspace:*"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
